{{#each uploads as |upload|}}
  <div class="card" id="upload-card" style="width: 18rem;">
    <img class="card-img-top" src="{{upload.url}}" alt="Card image cap">
    <div class="card-body">
      <section data-id="{{upload.id}}">
        <h3 class="card-title" data-id="{{upload.id}}">{{upload.title}}</h3>
        <form class="edit-title" data-id="{{upload.id}}">
          <input type="string" name="title" placeholder="title">
          <button type="submit" class="btn btn-default">Edit</button>
        </form>
      </section>
    </div>
  </div>
{{/each}}
